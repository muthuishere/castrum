(ns status-im.cosmos.stores.validators
  (:require
    [oops.core :refer [gget ocall ocall+ oget oget+]]
    [re-frame.core :as re-frame]
    [re-frame.db :as re-frame.db]
    [status-im.utils.fx :as fx]
    ["@keplr-wallet/stores" :refer (ChainStore CosmosQueries QueriesStore CosmwasmQueries)]
    [status-im.utils.money :as money]))





(defn get-validators-observable-query [db]
  (-> (get-in db [:keplr-store :query-store])
      (ocall "get" (get-in db [:keplr-store :selected-chain-id]))
      (oget "cosmos")
      (oget "queryValidators")
      (.getQueryStatus (get-in db [:keplr-store :selected-validator-status]))))



(defn format-validators [response]
  (-> response
      (js->clj :keywordize-keys true)
      (get-in [:response :data :validators])))


(defn commission-from [{:keys [commission]}]
  (-> commission
      (get-in [:commission_rates :rate] "0")
      (js/parseInt)
      (.toFixed 2)
      (str " %")))

(defn format-tokens [{:keys [tokens]}]
  (money/to-fixed (money/wei-> :eth tokens) 2))
(defn to-validator [validator]
  {:identity         (get-in validator [:description :identity])
   :name             (get-in validator [:description :moniker])
   :commission-rate  (commission-from validator)
   :voting-power     nil
   :description      (get-in validator [:description :details])
   :tokens           (format-tokens validator)
   :status           (get-in validator [:status])
   :delegator-shares (get-in validator [:delegator_shares])
   :operator-address (get-in validator [:operator_address])})

(defn get-all-validator-ids [response]
  (->> response
       (filter #(not-empty (get-in % [:description :identity])))
       (map (fn [validator]
              (get-in validator [:description :identity])))
       (into [])))



(fx/defn update-validators
         {:events [:keplr-store/updatevalidators]}
         [{:keys [db]} response]
         {:db       (assoc-in db [:keplr-store :validators] (format-validators response))
          :dispatch [:validator-thumbnail-store/initialize-all (get-all-validator-ids (format-validators response))]})

(fx/defn fetch-validators
         {:events [:keplr-store/fetchvalidators]}
         [{:keys [db]} _]
         {:keplr-observable/fetchquery {:query      (get-validators-observable-query db)
                                        :on-success [:keplr-store/updatevalidators]
                                        :on-failure [:ui/show-error (str "Unable to fetch validators for ")]}})

;status should be either
;Unbonded, Unbonding, Bonded, Unspecified
(fx/defn set-validator-status
         {:events [:keplr-store/set-validator-status]}
         [{:keys [db]} status]
         {:db       (assoc-in db [:keplr-store :selected-validator-status] status)
          :dispatch [:keplr-store/fetchvalidators]})



(re-frame/reg-sub
  :keplr-store/validators
  :<- [:keplr-store]
  (fn [keplr-store]
    (map to-validator (:validators keplr-store))))

(re-frame/reg-sub
  :keplr-store/selected-validator-status
  :<- [:keplr-store]
  (fn [keplr-store]
    (:selected-validator-status keplr-store)))
(re-frame/reg-sub
  :keplr-store/available-validator-statuses
  :<- [:keplr-store]
  (fn [keplr-store]
    (:available-validator-statuses keplr-store)))

(re-frame/reg-sub
  :keplr-store/validator-by-operator-address
  :<- [:keplr-store/validators]
  (fn [validators [_ operator-address]]
    (->> validators
         (filter #(= operator-address (get-in % [:operator-address]))) first)))



